<script>
function validateMAIN(){  
var name=document.f1.uname.value;  
var password=document.f1.pass.value;
   
  
if (name==null || name==""){  
  alert("Name can't be blank");  
  return false;  
}   
else if(password==null || password==""){
	alert("password field can't be blank");
	return false;
}
}  
</script>
